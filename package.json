{
    "name": "hska",
    "version": "1.0.0",
    "scripts": {
        "ng": "ng",
        "prestart": "npm run prettier",
        "start": "ng serve --port=443 --ssl --ssl-key=config/webserver/https/key.pem --ssl-cert=config/webserver/https/certificate.cer",
        "build": "ng build",
        "test": "ts-node -P scripts/tsconfig.json scripts/nightwatch",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "chromedriver": ".\\node_modules\\chromedriver\\lib\\chromedriver\\chromedriver.exe --verbose --port=9515",
        "clean": "ts-node -P scripts/tsconfig.json scripts/clean",
        "doc": "typedoc --verbose --options typedoc.json src",
        "fixme": "ts-node -P scripts/tsconfig.json scripts/fixme",
        "nginx": "ts-node -P scripts/tsconfig.json scripts/nginx",
        "prettier": "npx prettier --write src/**/*.ts scripts/*.ts test/**/*.js",
        "proxy": "ts-node -P scripts/tsconfig.json scripts/proxy",
        "selenium": "java -jar config\\selenium\\selenium-server-standalone.jar",
        "presource-map-explorer": "ng build --source-map",
        "source-map-explorer": "npx source-map-explorer dist/hska/main.js",
        "prewebpack-bundle-analyzer": "ng build --stats-json",
        "webpack-bundle-analyzer": "npx webpack-bundle-analyzer dist/hska/stats.json"
    },
    "private": true,
    "homepage": "https://www.hs-karlsruhe.de",
    "keywords": [
        "Angular",
        "TypeScript",
        "npm",
        "Bootstrap",
        "FontAwesome",
        "Chart.js",
        "Moment",
        "Nightwatch"
    ],
    "author": "Gruppe 6",
    "license": "GPL-3.0",
    "dependencies": {
        "@angular/animations": "7.1.0-rc.0",
        "@angular/common": "7.1.0-rc.0",
        "@angular/compiler": "7.1.0-rc.0",
        "@angular/core": "7.1.0-rc.0",
        "@angular/forms": "7.1.0-rc.0",
        "@angular/http": "7.1.0-rc.0",
        "@angular/platform-browser": "7.1.0-rc.0",
        "@angular/platform-browser-dynamic": "7.1.0-rc.0",
        "@angular/router": "7.1.0-rc.0",
        "@fortawesome/angular-fontawesome": "0.3.0",
        "@fortawesome/fontawesome-svg-core": "1.2.8",
        "@fortawesome/free-solid-svg-icons": "5.5.0",
        "bootstrap": "4.1.3",
        "chart.js": "2.7.3",
        "core-js": "2.5.7",
        "jquery": "3.3.1",
        "moment": "2.22.2",
        "popper.js": "1.14.5",
        "pwacompat": "2.0.7",
        "rxjs": "6.3.3",
        "zone.js": "0.8.26"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "0.11.0-rc.0",
        "@angular/cli": "7.1.0-rc.0",
        "@angular/compiler-cli": "7.1.0-rc.0",
        "@angular/language-service": "7.1.0-rc.0",
        "@babel/core": "7.1.6",
        "@babel/preset-env": "7.1.6",
        "@babel/register": "7.0.0",
        "@types/bootstrap": "4.1.2",
        "@types/chart.js": "2.7.40",
        "@types/inquirer": "0.0.43",
        "@types/jasmine": "2.8.11",
        "@types/jasminewd2": "2.0.6",
        "@types/minimist": "1.2.0",
        "@types/nightwatch": "0.9.11",
        "@types/node": "10.12.9",
        "@types/rimraf": "2.0.2",
        "@types/shelljs": "0.8.0",
        "babel-plugin-add-module-exports": "1.0.0",
        "chromedriver": "2.43.1",
        "codelyzer": "4.5.0",
        "fixme": "0.5.0",
        "inquirer": "6.2.0",
        "jasmine-core": "~2.99.1",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~3.0.0",
        "karma-chrome-launcher": "~2.2.0",
        "karma-coverage-istanbul-reporter": "~2.0.1",
        "karma-jasmine": "~1.1.2",
